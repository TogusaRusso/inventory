<!DOCTYPE html>
<html>
<head>
  <title>Inventory</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <tbody>
    <tr>
      <td><%= link_to 'Позиции', positions_path %></td>
      <td><%= link_to 'Серийные предметы', items_path %></td>
      <td><%= link_to 'МОЛы', people_path %></td>
      <td><%= link_to 'Документы', documents_path %></td>
      <td><%= link_to 'Проводки', movements_path %></td>
    </tr>
  </tbody>

<%= yield %>

<%= debug(params) if Rails.env.development? %>

</body>
</html>
